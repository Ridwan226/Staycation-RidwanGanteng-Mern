<% if(alert.message != "") { %>
<div
  class="alert alert-<%= alert.status %> alert-dismissible fade show"
  role="alert"
>
  <strong>Holy guacamole!</strong> <%= alert.message %>
  <button type="button" class="close" data-dismiss="alert" aria-label="Close">
    <span aria-hidden="true">&times;</span>
  </button>
</div>
<% }%>
